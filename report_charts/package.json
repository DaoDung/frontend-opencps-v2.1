{
  "name": "bbat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "build:rp1": "vue-cli-service build --dest dist/report_1 src/mainrp1.js",
    "build:rp2": "vue-cli-service build --dest dist/report_2 src/mainrp2.js",
    "build:rp3": "vue-cli-service build --dest dist/report_3 src/mainrp3.js",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-rc.1",
    "@inotom/vue-go-top": "^1.0.2",
    "apexcharts": "^2.2.4",
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "chart.piecelabel.js": "^0.12.0",
    "roboto-fontface": "*",
    "vue": "2.5.17",
    "vue-apexcharts": "^1.2.6",
    "vue-chartjs": "^3.4.0",
    "vue-content-placeholders": "^0.2.1",
    "vue-friendly-iframe": "^0.9.0",
    "vue-router": "^3.0.1",
    "vuetify": "1.3.11",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "babel-plugin-transform-imports": "^1.5.0",
    "vue-cli-plugin-vuetify": "^0.2.1",
    "vue-template-compiler": "2.5.17",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
